<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="CSS" injector-id="xml">
      <display-name>div</display-name>
      <place disabled="true"><![CDATA[xmlTag().withLocalName(string().equalTo("div")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
    <injection language="TEXT" injector-id="xml">
      <display-name>html</display-name>
      <place disabled="true"><![CDATA[xmlTag().withLocalName(string().equalTo("html")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>